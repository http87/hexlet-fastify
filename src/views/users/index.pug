extends ../layouts/page.pug

block content
  table.table.table-striped
    tr
      th Идентификатор пользователя
      th Имя пользователя
      th Емейл пользователя
    each user, index in users
      tr
        td
          a(href='/users/' + user.id) #{user.id}
        td= user.username
        td= user.email